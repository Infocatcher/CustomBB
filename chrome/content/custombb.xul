				</menu>

				<menuitem id="custombb-popup-custom-b-1" line1="&custombb.tooltip.custom;:" class="menuitem-iconic custombb-mi" cbbeditable="menuitem" onclick="custombb.clickHandler(event, true);" oncommand="custombb.clickHandler(event, true);" />
				<menuitem id="custombb-popup-custom-b-2" line1="&custombb.tooltip.custom;:" class="menuitem-iconic custombb-mi" cbbeditable="menuitem" onclick="custombb.clickHandler(event, true);" oncommand="custombb.clickHandler(event, true);" />
